<div class="col-sm-4" ng-controller="LoginCtrl">
    <h3>Log In</h3>
    <div class="validation-summary-errors" ng-show="errorData.isError">
        <span class="glyphicon glyphicon-exclamation-sign">{{errorData.errorMessage}}</span>
    </div>
    <form>
        <div class="form-group">
            <label>Email</label>
            <input class="form-control" type="text" ng-model="loginData.userName" />
        </div>
        <div class="form-group">
            <label>Password</label>
            <input class="form-control" type="password" ng-model="loginData.password" />
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-default" ng-click="login()">Log In</button>
        </div>
    </form>
</div>
